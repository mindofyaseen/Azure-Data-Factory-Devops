{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "yaseenazuredatafactory"
		},
		"ls_yaseendatafactorystorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_yaseendatafactorystorage'"
		},
		"ls_yaseendfsqlserver_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_yaseendfsqlserver'"
		},
		"ls_rest_api_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://pokeapi.co/api/v2/pokemon"
		},
		"ls_yaseendatafactorystorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://yaseendatafactorystorage.dfs.core.windows.net/"
		},
		"ls_yaseendfsqlserver_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "yaseendfsqlserver.database.windows.net"
		},
		"ls_yaseendfsqlserver_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "yaseenDB"
		},
		"ls_yaseendfsqlserver_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "yaseen"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "yaseenazuredatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_yaseendatafactorystorage_accountKey": {
						"value": "[parameters('ls_yaseendatafactorystorage_accountKey')]"
					},
					"ls_yaseendfsqlserver_password": {
						"value": "[parameters('ls_yaseendfsqlserver_password')]"
					},
					"ls_rest_api_properties_typeProperties_url": {
						"value": "[parameters('ls_rest_api_properties_typeProperties_url')]"
					},
					"ls_yaseendatafactorystorage_properties_typeProperties_url": {
						"value": "[parameters('ls_yaseendatafactorystorage_properties_typeProperties_url')]"
					},
					"ls_yaseendfsqlserver_properties_typeProperties_server": {
						"value": "[parameters('ls_yaseendfsqlserver_properties_typeProperties_server')]"
					},
					"ls_yaseendfsqlserver_properties_typeProperties_database": {
						"value": "[parameters('ls_yaseendfsqlserver_properties_typeProperties_database')]"
					},
					"ls_yaseendfsqlserver_properties_typeProperties_userName": {
						"value": "[parameters('ls_yaseendfsqlserver_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}